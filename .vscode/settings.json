{
  "code-runner.showRunIconInEditorTitleMenu": true,
  "code-runner.executorMapByFileExtension": {
    ".epp": "python \"$workspaceRoot/epp_runner.py\" \"$fullFileName\""
  },
  "code-runner.runInTerminal": true,
  "files.associations": {
    "*.epp": "epp"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "source.epp keyword.control.flow.epp"
        ],
        "settings": {
          "foreground": "#FF6B6B",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "source.epp keyword.command.epp"
        ],
        "settings": {
          "foreground": "#FF7A59",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "source.epp keyword.control.statement.epp",
          "source.epp keyword.operator.epp"
        ],
        "settings": {
          "foreground": "#F4A261"
        }
      },
      {
        "scope": [
          "source.epp support.function.builtin.epp",
          "source.epp entity.name.function.epp"
        ],
        "settings": {
          "foreground": "#4FC3F7",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "source.epp variable.other.assignment.epp"
        ],
        "settings": {
          "foreground": "#FFD166"
        }
      },
      {
        "scope": [
          "source.epp constant.numeric.epp"
        ],
        "settings": {
          "foreground": "#B392F0"
        }
      },
      {
        "scope": [
          "source.epp constant.language.boolean.epp"
        ],
        "settings": {
          "foreground": "#7BD389",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "source.epp string.quoted.double.epp",
          "source.epp string.quoted.single.epp"
        ],
        "settings": {
          "foreground": "#98C379"
        }
      },
      {
        "scope": [
          "source.epp comment.line.number-sign.epp"
        ],
        "settings": {
          "foreground": "#5C6370",
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "source.epp support.type.web.epp"
        ],
        "settings": {
          "foreground": "#E06C75",
          "fontStyle": "bold"
        }
      }
    ]
  }
}
